
{
    "presets": ["next/babel"],
    "plugins": [
      
      [
        "module-resolver",
        {
          "alias": {
            "@/components": "./components",
            "@/constants": "./constants",
            "@/pages": "./pages",
            "@/public": "./public",
            "@/reducers": "./reducers",
            "@/utils": "./utils",
            "@/services":"./services",
            "@/imge":"./images",
            "@/assets":"./assets"
          }
        },
       "@babel/plugin-transform-runtime"
       
        
      ],
      [
        "styled-components", 
        { "ssr": true }
      ]
    ]
  }